#!/bin/bash
# var
python main.py --activation relu --epoch 500 --network GMM_MEDIUM_RELU --dataset GMM_CIRCLE --init_type xavier --gmm_cluster 4 --gmm_scale 2 --gmm_dim 2 --sample_size 512 --batch_size 512 --is_train 1 --is_crop False --g_heruistic 1 --g_update 1 --z_dim 256 --c_dim 1 --visualize_interval 10 --learning_rate_d 1e-4 --learning_rate_g 1e-4 --log_dir logs_128_relu_c_4 --sample_dir samples_128_relu_c_4 --checkpoint_dir checkpoint_128_relu_c_4 --gmm_var 0.02
python main.py --activation tanh --epoch 500 --network GMM_MEDIUM_RELU --dataset GMM_CIRCLE --init_type xavier --gmm_cluster 4 --gmm_scale 2 --gmm_dim 2 --sample_size 512 --batch_size 512 --is_train 1 --is_crop False --g_heruistic 1 --g_update 1 --z_dim 256 --c_dim 1 --visualize_interval 10 --learning_rate_d 1e-4 --learning_rate_g 1e-4 --log_dir logs_128_tanh_c_4 --sample_dir samples_128_tanh_c_4 --checkpoint_dir checkpoint_128_tanh_c_4 --gmm_var 0.02
